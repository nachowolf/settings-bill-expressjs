<div class="row">

    <h1>Actions</h1>

    <table>
        <tr>
            <th>Type</th>
            <th>Price</th>
            <th>When</th>
        </tr>
        {{#each actions}}
        <tr>
            <td>{{type}}</td>
            <td>{{price}}</td>
            <td>{{timeStamp}}</td>
            {{/each}}
        </tr>
    </table>

</div>